{
    "manifest_version": 3,
    "name": "AD Finder",
    "description": "Providing advertising blog display",
    "version": "1.0",
    "background" : {
        "service_worker" : "background.js"

    },

    "icons" : {
        "128": "icon_sw.png"
    },

    "action": {
      "default_icon": {
        "128": "icon_sw.png"
      }
    },
    "permissions": ["activeTab", "scripting"],
    "content_security_policy" : {
      "extension_pages": "script-src 'self'; object-src 'self';"}
    }