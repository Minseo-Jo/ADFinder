{
    "manifest_version": 3,
    "name": "Naver_blog",
    "description": "advertisement perception",
    "version": "1.0",
    "background" : {
        "service_worker" : "background.js"

    },
    "content_scripts": [
        {
          "js": ["scripts/content_script.js"],
          "matches": [
            "https://search.naver.com/*"
          ]
        }
      ],
    "icons" : {
        "128": "icon_sw.png"
    },

    "action": {
      "default_icon": {
        "128": "icon_sw.png"
      }
    },
    "permissions": ["activeTab", "scripting"]
}





 